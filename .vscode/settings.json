{
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=.",
                "-jobname=cheatsheet",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "cSpell.words": [
        "checkpointing",
        "explus",
        "FCFS",
        "Interdomain",
        "MITM",
        "multicol",
        "parallelisable",
        "QUIC",
        "SRTF",
        "Traceroute"
    ]
}